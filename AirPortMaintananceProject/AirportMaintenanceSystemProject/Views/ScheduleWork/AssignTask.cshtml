
@model AirportMaintenanceSystemProject.Models.AssignTask

@{
    ViewBag.Title = "AssignTask";
}
<head>
    <link rel="stylesheet" href="@Url.Content("/Content/Delete.css")" />
</head>
<div class="form-horizontal">
    <div class="heading">
        <h2>DeleteOperation</h2>
        <h3>Are you sure you want to delete this?</h3>
    </div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-primary" /> |
            @Html.ActionLink("Back to List", "AssignTaskList", new { @class = "btn btn-primary" })
        </div>
    }
</div>
